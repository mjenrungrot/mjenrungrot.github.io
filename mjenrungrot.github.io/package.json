{
  "name": "mjenrungrot.github.io",
  "homepage": "https://mjenrungrot.github.io",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.5.0",
    "@material-ui/icons": "^4.4.3",
    "@material-ui/styles": "^4.5.0",
    "@webpack-cli/init": "^0.2.2",
    "babel-loader": "^8.0.4",
    "classnames": "^2.2.6",
    "clean-webpack-plugin": "^1.0.1",
    "css-loader": "^2.1.1",
    "eslint-loader": "^2.2.1",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-prettier": "^3.1.1",
    "file-loader": "^3.0.1",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "jest-enzyme": "^7.1.1",
    "markdown-loader": "^5.1.0",
    "markdown-to-jsx": "^6.10.3",
    "node-sass": "^4.12.0",
    "path": "^0.12.7",
    "prettier": "^1.18.2",
    "prettier-eslint": "^9.0.0",
    "prop-types": "^15.7.2",
    "react": "^16.10.2",
    "react-dom": "^16.10.2",
    "react-icons": "^3.7.0",
    "react-router-dom": "^5.1.2",
    "style-loader": "^0.23.1",
    "typeface-roboto": "0.0.54",
    "webpack": "^4.41.1",
    "webpack-cli": "^3.3.9",
    "webpack-dev-server": "^3.8.2"
  },
  "scripts": {
    "flow": "flow",
    "lint": "eslint src/**/*.js",
    "format": "npm run lint -- --fix",
    "check-all": "npm run lint && npm run flow",
    "start": "webpack-dev-server --open --config webpack.dev.js",
    "build": "webpack --config webpack.prod.js",
    "test": "jest",
    "deploy": "gh-pages -b master -d build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --no-ignore --fix",
      "git add"
    ],
    "*.json": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ],
  "jest": {
    "setupFiles": [
      "./src/setupTests.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "moduleNameMapper": {
      "\\.(pdf|jpg|ico|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "<rootDir>/__mocks__/styleMock.js"
    }
  },
  "devDependencies": {
    "@babel/core": "^7.6.2",
    "@babel/plugin-proposal-class-properties": "^7.5.5",
    "@babel/preset-env": "^7.6.2",
    "@babel/preset-react": "^7.0.0",
    "@typescript-eslint/eslint-plugin": "^2.3.3",
    "@typescript-eslint/parser": "^2.3.3",
    "babel-eslint": "^10.0.0",
    "babel-jest": "^24.9.0",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "enzyme-to-json": "^3.4.2",
    "eslint": "^6.5.1",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.16.0",
    "flow-bin": "^0.97.0",
    "gh-pages": "^2.1.1",
    "husky": "^1.3.1",
    "jest": "^24.9.0",
    "lint-staged": "^9.4.2",
    "webpack-merge": "^4.2.2"
  }
}
